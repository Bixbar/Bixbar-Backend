input-view {
  match {
    Age (age) { 
      to-input: SubmitSurvey
    }
  }
  message {
    template ("What is your gender?")
  }
  render {
    form {
      elements {
        number-input {
          id (age)
          allow-negative (false)
          integer-limit (3)
          type (Age)
          label (Age)
        }
      }
      on-submit {
        goal: Age
        value: viv.core.FormElement(age)
      }
    }
  }
}
