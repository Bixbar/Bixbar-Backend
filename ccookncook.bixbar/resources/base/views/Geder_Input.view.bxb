input-view {
  match {
    Gender (gender) { 
      to-input: SubmitSurvey
    }
  }
  message {
    template ("What is your gender?(Female/Male)")
  }
  render {
    form {
      elements {
        text-input {
          id (gender)
          label (Gender)
          type (Gender)
          max-length (10)
          value ("#{raw(gender)}")
        }
      }
      on-submit {
        goal: Gender
        value: viv.core.FormElement(gender)
      }
    }
  }
}